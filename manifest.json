{
    "name": "Auto Fork",
    "version": "2.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "browser_button.html",
        "default_title": "Auto Fork"
    }
}